    "isbot": "^3.7.0",
    "koa": "^2.14.2",
    "koa-static": "^5.0.0",
    "koa-trust-proxy": "^0.1.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-select": "^5.7.7",
    "zod": "^3.22.4"
