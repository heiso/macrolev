{
  "name": "web-app",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "build": "cross-env NODE_ENV=production react-router build",
    "preview": "react-router preview",
    "dev": "react-router dev",
    "start": "cross-env NODE_ENV=production react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc && && eslint . && prettier --check .",
    "check-dependencies": "npx npm-check-updates --peer --format group -i",
    "down": "pm2 delete ecosystem.config.cjs",
    "log": "pm2 log",
    "restart": "pm2 delete ecosystem.config.cjs && pm2 start ecosystem.config.cjs",
    "up": "pm2 start ecosystem.config.cjs"
  },
  "dependencies": {
    "@react-router/node": "^7.1.1",
    "@react-router/serve": "^7.1.1",
    "isbot": "^5.1.17",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "^7.1.1"
  },
  "devDependencies": {
    "@react-router/dev": "^7.1.1",
    "@types/node": "^20",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.1",
    "@types/w3c-web-hid": "^1.0.6",
    "@types/w3c-web-serial": "^1.0.7",
    "@types/w3c-web-usb": "^1.0.10",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "pm2": "^5.4.3",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "prettier-plugin-organize-imports": "^4.1.0",
    "tailwindcss": "^3.4.16",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vite": "^5.4.11",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
